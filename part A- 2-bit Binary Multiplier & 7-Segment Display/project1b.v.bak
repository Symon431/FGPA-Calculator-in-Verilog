module project1b(
    input wire [1:0] A,  // 2-bit input A
    input wire [1:0] B,  // 2-bit input B
    output wire [6:0] seg  // 7-segment display output
);
    // Internal wire for the multiplier output
    wire [3:0] C;

    // Instantiate the 2-bit binary multiplier
    binary_multiplier mult (
        .A(A),  // Connect input A
        .B(B),  // Connect input B
        .C(C)   // Connect output C
    );

    // Instantiate the 7-segment decoder
    seg_decoder dec (
        .num(C),  // Connect multiplier output C to decoder input
        .seg(seg)  // Connect decoder output to seg
    );
endmodule